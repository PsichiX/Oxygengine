{
  "commands": [
    {
      "Pipeline": {
        "source": "../assets",
        "destination": "../assets-intermediate",
        "clear_destination": true,
        "commands": [
          {
            "Plugin": {
              "name": "oxygengine-ha-renderer-spine",
              "params": {
                "input_documents": [
                  "skeletons/watcher.json"
                ],
                "output": "skeletons/",
                "assets_path_prefix": "skeletons/"
              }
            }
          },
          {
            "Copy": {
              "from": [
                "assets.txt"
              ]
            }
          }
        ]
      }
    },
    {
      "Pipeline": {
        "destination": "../assets-baked",
        "clear_destination": true,
        "commands": [
          {
            "Pack": {
              "paths": [
                "../assets-intermediate/"
              ],
              "output": "assets.pack"
            }
          }
        ]
      }
    }
  ]
}